=> {\n        this.hitokoto = data.hitokoto;\n        this.from = data.from;\n        console.log(data);\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    getMusic() {\n      // const ap = new APlayer({\n      // container: document.getElementById('aplayer'),\n      // audio: [{\n      //     name: 'name',\n      //     artist: 'artist',\n      //     url: 'url.mp3',\n      //     cover: 'cover.jpg'\n      // }]\n      // });\n      // console.log(ap);\n    }\n  }\n};","map":{"version":3,"names":["name","components","data","backgroundImage","currentImagePath","require","imagePaths","city","weather","windDirection","windScale","weatherDate","hitokoto","from","mounted","loadingBoxDom","document","getElementById","getBgImg","getWeather","getHoko","getMusic","setInterval","getTime","setTimeout","style","display","methods","gotoUrl","type","window","open","location","href","randomIndex","Math","floor","random","length","add_id","app_secret","key","fetch","then","response","json","replace","console","log","id","weatherData","now","windDir","catch","error","date","Date","year","getFullYear","month","getMonth","slice","day","getDate","hours","getHours","minutes","getMinutes","seconds","getSeconds","weekday","getDay","formattedTime"],"sources":["E:\\current-project\\wwk_blog\\src\\App.vue"],"sourcesContent":["<template>\n<div class=\"body\">\n  <div id=\"loading-box\">\n    <div class=\"loading-left-bg\"></div>\n    <div class=\"loading-right-bg\"></div>\n    <div class=\"spinner-box\">\n        <div class=\"loader\">\n            <div class=\"inner one\"></div>\n            <div class=\"inner two\"></div>\n            <div class=\"inner three\"></div>\n            <div class=\"inner four\"></div>\n        </div>\n        <div class=\"loading-word\">\n            <p class=\"loading-title\" id=\"loading-title\">Anchorの主页</p>\n            <span id=\"loading-text\">加载中</span>\n        </div>\n    </div>\n</div>\n    <!-- 鼠标指针\n    <div id=\"g-poi