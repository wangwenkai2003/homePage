{"ast":null,"code":"export default {\n  name: 'App',\n  components: {},\n  data() {\n    return {\n      backgroundImage: `url(${this.currentImagePath})`,\n      currentImagePath: require('@/static/img/background1.webp'),\n      imagePaths: [require('@/static/img/background1.webp'), require('@/static/img/background2.webp'), require('@/static/img/background3.webp'), require('@/static/img/background4.webp'), require('@/static/img/background5.webp'), require('@/static/img/background6.webp')],\n      city: '',\n      weather: {},\n      windDirection: '',\n      windScale: ''\n    };\n  },\n  mounted() {\n    const loadingBoxDom = document.getElementById('loading-box');\n    this.getBgImg();\n    this.getWeather();\n    setTimeout(() => {\n      loadingBoxDom.style.display = 'none';\n    }, 200);\n  },\n  methods: {\n    getBgImg() {\n      // 获取一个随机索引\n      const randomIndex = Math.floor(Math.random() * this.imagePaths.length);\n      // 设置当前背景图片路径\n      this.currentImagePath = this.imagePaths[randomIndex];\n    },\n    getWeather() {\n      const add_id = \"rvjnn0fyqo0kstgn\"; // app_id\n      const app_secret = \"ytQ289YY6NFYVHgN5QOQHchCSoz7kCyO\"; // app_secret\n      const key = \"39380313fb85496b8269140866349c1d\"; // key\n      fetch(\"https://www.mxnzp.com/api/ip/self?app_id=\" + add_id + \"&app_secret=\" + app_secret).then(response => response.json()).then(data => {\n        this.city = data.data.city.replace(/市/g, '');\n        console.log(this.city, 'tc');\n        fetch(\"https://geoapi.qweather.com/v2/city/lookup?location=\" + this.city + \"&number=1&key=\" + key).then(response => response.json()).then(location => {\n          const id = location.location[0].id;\n          fetch(\"https://devapi.qweather.com/v7/weather/now?location=\" + id + \"&key=\" + key).then(response => response.json()).then(weatherData => {\n            this.weather = weatherData.now;\n            this.windDirection = weatherData.now.windDir;\n            this.windScale = weatherData.now.windScale;\n            console.log(this.weather, 'weather');\n            console.log(this.windDirection, 'weatherD');\n            console.log(this.windScale, 'weatherS');\n          });\n        }).catch(console.error);\n      }).catch(console.error);\n    }\n  }\n};","map":{"version":3,"names":["name","components","data","backgroundImage","currentImagePath","require","imagePaths","city","weather","windDirection","windScale","mounted","loadingBoxDom","document","getElementById","getBgImg","getWeather","setTimeout","style","display","methods","randomIndex","Math","floor","random","length","add_id","app_secret","key","fetch","then","response","json","replace","console","log","location","id","weatherData","now","windDir","catch","error"],"sources":["E:\\current-project\\wwk_blog\\src\\App.vue"],"sourcesContent":["<template>\n<div class=\"body\">\n  <div id=\"loading-box\">\n    <div class=\"loading-left-bg\"></div>\n    <div class=\"loading-right-bg\"></div>\n    <div class=\"spinner-box\">\n        <div class=\"loader\">\n            <div class=\"inner one\"></div>\n            <div class=\"inner two\"></div>\n            <div class=\"inner three\"></div>\n            <div class=\"inner four\"></div>\n        </div>\n        <div class=\"loading-word\">\n            <p class=\"loading-title\" id=\"loading-title\">Anchorの主页</p>\n            <span id=\"loading-text\">加载中</span>\n        </div>\n    </div>\n</div>\n    <!-- 鼠标指针\n    <div id=\"g-pointer-1\"></div>\n    <div id=\"g-pointer-2\"></div> -->\n    <!-- 主体内容 -->\n    <main id=\"main\" class=\"main\" :style=\"{ backgroundImage: `url(${currentImagePath})` }\">\n\n                    <!--基本信息-->\n                    <div class=\"main-left\" style=\"width: 45%;\">\n                        <!--Logo-->\n                        <div class=\"main-img\">\n                            <img id=\"logo-img\" src=\"./assets/yumaoqiu.jpg\" alt=\"img\">\n                            <div style=\"display: flex;flex-direction: column; justify-content: center;\">\n                                <div>\n                                <span class=\"img-title-big\" id=\"logo-text-1\">WangWK</span>\n                                <span class=\"img-text\" id=\"logo-text-2\">.top</span>\n                                 </div>\n                                 <div style=\"margin-left: 30px\">\n                                    <span class=\"signature\">\n                                        Love can offset the long years\n                                    </span>\n                                 </div>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"main-right\" style=\"width: 55%; margin-top: 20px;\">\n                        <!-- 功能区 -->\n                        <div style=\"width: 100%;display: flex;\">\n                            <div class=\"col hitokotos\" >\n                                <!--一言-->\n                                <div class=\"hitokoto cards\" id=\"hitokoto\">\n                                    <!--切换音乐-->\n                                    <div class=\"open-music fixed-top\" id=\"open-music\">\n                                        <i class=\"fa-solid fa-compact-disc\"></i>\n                                        <span>&nbsp;打开音乐播放器</span>\n                                    </div>\n                                    <!--切换音乐结束-->\n                                    <div class=\"hitokoto-all\">\n                                        <div class=\"hitokoto-text\"><span\n                                                id=\"hitokoto_text\">每一个人都应该明确自己的方向和位置</span>\n                                        </div>\n                                        <div class=\"hitokoto-from\">-「&nbsp;<span id=\"from_text\">無名</span>&nbsp;」\n                                        </div>\n                                    </div>\n                                </div>\n                                <!--音乐-->\n                                <div class=\"music\" id=\"music\">\n                                    <div class=\"music-all\">\n                                        <div class=\"music-button\">\n                                            <div id=\"music-open\">音乐列表</div>\n                                            <div id=\"music-close\">回到一言</div>\n                                        </div>\n                                        <div class=\"music-control\">\n                                            <i class=\"fa-solid fa-backward-step\" id=\"last\"></i>\n                                            <div id=\"play\">\n                                                <i class=\"fa-solid fa-play\"></i>\n                                            </div>\n                                            <i class=\"fa-solid fa-forward-step\" id=\"next\"></i>\n                                        </div>\n                                        <div class=\"music-menu\">\n                                            <div class=\"music-text\">\n                                                <span id=\"music-name\">未播放音乐</span>\n                                            </div>\n                                            <div class=\"music-volume\" style=\"display: none;\">\n                                                <div id=\"volume-ico\">\n                                                    <i class=\"fa-solid fa-volume-low\"></i>\n                                                </div>\n                                                <input type=\"range\" min=\"0\" max=\"1\" step=\"0.01\" id=\"volume\">\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"col times\" style=\"width: 45%;\">\n                                <!--时间-->\n                                <div class=\"time cards\" id=\"upWeather\">\n                                    <div class=\"timeshow\" id=\"time\">\n                                        2000&nbsp;年&nbsp;0&nbsp;月&nbsp;00&nbsp;日&nbsp;<span\n                                            class=\"weekday\">星期一</span><br><span class=\"time-text\">00:00:00</span>\n                                    </div>\n                                    <div class=\"weather\">\n                                        <span id=\"city_text\">{{ city }}</span>&nbsp;\n                                        <span id=\"wea_text\">{{weather.text}}</span>&nbsp;\n                                        <span id=\"tem_text\">{{ weather.temp }}</span>\n                                        <span id=\"win_text \" style=\"margin-left: 2px;\">{{ weather.windDir }}</span>\n                                        <span id=\"win_speed\">超限</span>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n\n                    </div>\n\n    </main>\n</div>\n</template>\n\n<script>\nexport default {\n  name: 'App',\n  components: {\n  },\n  data() {\n    return {\n      backgroundImage: `url(${this.currentImagePath})`,\n      currentImagePath: require('@/static/img/background1.webp'),\n      imagePaths: [\n      require('@/static/img/background1.webp'),\n      require('@/static/img/background2.webp'),\n      require('@/static/img/background3.webp'),\n      require('@/static/img/background4.webp'),\n      require('@/static/img/background5.webp'),\n      require('@/static/img/background6.webp')],\n      city: '',\n      weather: {},\n      windDirection: '',\n      windScale: ''\n    };\n  },\n  mounted() {\n    const loadingBoxDom = document.getElementById('loading-box');\n    this.getBgImg()\n    this.getWeather()\n    setTimeout(()=>{\n        loadingBoxDom.style.display = 'none';\n    },200)\n  },\n  methods: {\n    getBgImg(){\n      // 获取一个随机索引\n      const randomIndex = Math.floor(Math.random() * this.imagePaths.length);\n      // 设置当前背景图片路径\n      this.currentImagePath = this.imagePaths[randomIndex];\n    },\n    getWeather() {\n        const add_id = \"rvjnn0fyqo0kstgn\"; // app_id\n        const app_secret = \"ytQ289YY6NFYVHgN5QOQHchCSoz7kCyO\"; // app_secret\n        const key = \"39380313fb85496b8269140866349c1d\" // key\n        fetch(\"https://www.mxnzp.com/api/ip/self?app_id=\" + add_id + \"&app_secret=\" + app_secret)\n      .then(response => response.json())\n      .then(data => {\n        this.city = data.data.city.replace(/市/g, '');\n        console.log(this.city,'tc');\n\n        fetch(\"https://geoapi.qweather.com/v2/city/lookup?location=\" + this.city + \"&number=1&key=\" + key)\n          .then(response => response.json())\n          .then(location => {\n            const id = location.location[0].id;\n            fetch(\"https://devapi.qweather.com/v7/weather/now?location=\" + id + \"&key=\" + key)\n              .then(response => response.json())\n              .then(weatherData => {\n                this.weather = weatherData.now;\n                this.windDirection = weatherData.now.windDir;\n                this.windScale = weatherData.now.windScale;\n                console.log(this.weather,'weather');\n                console.log( this.windDirection,'weatherD');\n                console.log( this.windScale,'weatherS');\n              })\n          })\n          .catch(console.error);\n      })\n      .catch(console.error);\n    }\n  }\n}\n\n\n</script>\n\n<style>\n*{\n  padding: 0;\n  margin: 0;\n  box-sizing: border-box;\n}\n</style>\n<style scoped src=\"@/static/css/loading.css\"></style>\n<style scoped src=\"@/static/css/animation.css\"></style>\n<style scoped src=\"@/static/css/lantern.css\"></style>\n<style scoped src=\"@/static/css/mobile.css\"></style>\n<style scoped src=\"@/static/css/style.css\"></style>\