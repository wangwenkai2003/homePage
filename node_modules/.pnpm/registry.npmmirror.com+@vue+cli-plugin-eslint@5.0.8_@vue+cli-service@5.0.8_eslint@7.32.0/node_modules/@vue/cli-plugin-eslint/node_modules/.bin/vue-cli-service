#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules/@vue/cli-service/bin/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules/@vue/cli-service/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules/@vue/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules/@vue/cli-service/bin/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules/@vue/cli-service/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules/@vue/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules/@vue/cli-service/bin/vue-cli-service.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+@vue+cli-service@5.0.8_vue@3.4.21/node_modules/@vue/cli-service/bin/vue-cli-service.js" "$@"
fi
