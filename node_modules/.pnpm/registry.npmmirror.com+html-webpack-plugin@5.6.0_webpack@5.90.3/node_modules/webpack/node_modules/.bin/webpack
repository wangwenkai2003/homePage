#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+webpack@5.90.3/node_modules/webpack/bin/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+webpack@5.90.3/node_modules/webpack/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+webpack@5.90.3/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+webpack@5.90.3/node_modules/webpack/bin/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+webpack@5.90.3/node_modules/webpack/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/registry.npmmirror.com+webpack@5.90.3/node_modules:/mnt/e/current-project/wwk_blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/webpack.js" "$@"
else
  exec node  "$basedir/../../bin/webpack.js" "$@"
fi
